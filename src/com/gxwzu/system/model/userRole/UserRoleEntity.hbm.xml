<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gxwzu.system.model.userRole.UserRoleEntity" table="user_role" schema="gdm">
        <composite-id mapped="true" class="com.gxwzu.system.model.userRole.UserRoleEntityPK">
            <key-property name="roleId">
                <column name="roleId" sql-type="int(11)"/>
            </key-property>
            <key-property name="userHelpId">
                <column name="userHelpId" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>